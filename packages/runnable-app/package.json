{
  "name": "@runnablejs/app",
  "version": "0.6.0",
  "description": "Web application for Runnable - Build internal workflows with ease",
  "sideEffects": false,
  "type": "module",
  "license": "MIT",
  "author": "Kinetic",
  "homepage": "https://getrunnable.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/kineticio/runnable.git",
    "directory": "packages/runnable-app"
  },
  "bugs": {
    "url": "https://github.com/kineticio/runnable/issues"
  },
  "keywords": [
    "runnable",
    "app",
    "internal-tools",
    "workflows",
    "admin",
    "react-router"
  ],
  "main": "build/index.js",
  "types": "app/types.ts",
  "files": [
    "app/types.ts",
    "app/api",
    "build",
    "public"
  ],
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev",
    "dev:server": "react-router-serve build/server/index.js",
    "build:server": "esbuild --platform=node --format=cjs ./server/server.ts --outfile=build/server.cjs --bundle",
    "start:server": "cross-env RUNNABLE_BASE_URL='/' NODE_ENV=production node ./build/server.cjs",
    "prepublishOnly": "pnpm build",
    "lint": "oxlint src --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "typecheck": "react-router typegen && tsc --noEmit",
    "types": "tsc -p tsconfig.types.json"
  },
  "dependencies": {
    "@chakra-ui/react": "^3.30.0",
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.14.1",
    "@iconify-json/fa": "^1.2.2",
    "@iconify/react": "^4.1.1",
    "@react-router/express": "^7.10.0",
    "@react-router/fs-routes": "^7.10.0",
    "@react-router/node": "^7.10.0",
    "@react-router/serve": "^7.10.0",
    "@runnablejs/api": "workspace:*",
    "compression": "^1.8.1",
    "express": "^4.22.1",
    "express-prometheus-middleware": "^1.2.0",
    "isbot": "^5.1.32",
    "lodash.get": "^4.4.2",
    "lodash.set": "^4.3.2",
    "lucide-react": "^0.555.0",
    "morgan": "^1.10.1",
    "next-themes": "^0.4.6",
    "prom-client": "^14.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.5.0",
    "react-remove-scroll": "^2.7.2",
    "react-router": "^7.10.0",
    "remix-auth": "^4.2.0",
    "remix-auth-form": "^3.0.0",
    "remix-auth-google": "^2.0.0",
    "remix-auth-oauth2": "^3.4.1",
    "remix-utils": "^9.0.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "stylis": "^4.3.6",
    "tiny-invariant": "^1.3.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@chakra-ui/cli": "^3.30.0",
    "@react-router/dev": "^7.10.0",
    "@runnablejs/config": "workspace:*",
    "@types/compression": "^1.8.1",
    "@types/express": "^4.17.25",
    "@types/express-prometheus-middleware": "^1.2.3",
    "@types/lodash.get": "^4.4.9",
    "@types/lodash.set": "^4.3.9",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.10.1",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "@types/uuid": "^9.0.8",
    "@vitest/ui": "^4.0.15",
    "cross-env": "^7.0.3",
    "esbuild": "^0.27.1",
    "jsdom": "^27.2.0",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^3.14.0",
    "typescript": "^5.9.3",
    "vite": "^7.2.6",
    "vitest": "^4.0.15"
  }
}
