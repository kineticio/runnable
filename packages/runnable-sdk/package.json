{
  "name": "@runnablejs/sdk",
  "version": "0.6.0",
  "description": "JavaScript/TypeScript SDK for Runnable - Build internal workflows with ease",
  "type": "module",
  "sideEffects": false,
  "license": "MIT",
  "author": "Kinetic",
  "homepage": "https://getrunnable.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/kineticio/runnable.git",
    "directory": "packages/runnable-sdk"
  },
  "bugs": {
    "url": "https://github.com/kineticio/runnable/issues"
  },
  "keywords": [
    "runnable",
    "sdk",
    "internal-tools",
    "workflows",
    "admin"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "build:watch": "vite build --watch",
    "prepublishOnly": "pnpm build",
    "lint": "oxlint src --fix",
    "test": "vitest run --passWithNoTests || exit 0",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch"
  },
  "devDependencies": {
    "@runnablejs/api": "workspace:*",
    "socket.io-client": "^4.8.1",
    "typescript": "^5.9.3",
    "vite": "^7.2.6",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.15"
  }
}
