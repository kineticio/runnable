    "@types/morgan": "^1.9.10",
    "@types/node": "^24.10.1",
    "cookie-parser": "^1.4.7",
    "express": "^4.22.1",
    "resolve-package-path": "^4.0.3",
    "tsx": "^3.14.0",
    "typescript": "^5.9.3",
    "vite": "^7.2.6",
    "vite-plugin-dts": "^4.5.4"
  },
  "dependencies": {
    "@runnablejs/api": "workspace:*",
    "@runnablejs/app": "workspace:*",
    "@runnablejs/sdk": "workspace:*"
