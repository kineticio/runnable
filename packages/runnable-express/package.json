{
  "name": "@runnablejs/express",
  "version": "0.4.15",
  "type": "module",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && rollup -c",
    "build:watch": "rollup -c --watch --watch.include=src/**",
    "dev": "tsx src/dev/dev.ts",
