    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "devDependencies": {
    "@remix-run/express": "^1.19.3",
    "@runnablejs/config": "*",
    "@runnablejs/tsconfig": "*",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "cookie-parser": "^1.4.7",
    "express": "^4.21.1",
    "resolve-package-path": "^4.0.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^3.14.0",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@runnablejs/api": "*",
    "@runnablejs/app": "*",
    "@runnablejs/sdk": "*"
  }
